{
    "name": "AutoReloader",
    "description": "Injecting http-equiv=refresh into every opened tab",
    "homepage_url": "https://sieber.systems",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["inject.js"]
        }
    ],
    "host_permissions": ["<all_urls>"]
}